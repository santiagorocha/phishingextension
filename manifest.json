{
    "manifest_version": 2,
    "name": "Phishing detect extension 2",
    "version": "1.0",
    "content_scripts": [
        {
            "matches":
            [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": { 
        "browser_style": true,
        "default_icon": "ap.png",
        "default_popup": "https://www.virustotal.com/gui/",
        "default_title": "Antiphishing extension"
    }
}